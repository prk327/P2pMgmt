<% include ../partials/header %>

<% include ../partials/navbar %>

<div class="connectedData">
    <div class="connectionName">
        <h4><%= dSource %></h4>
        <div class="tablesNames">
            <ul>
                <%    let j = 0; %>
                <%    excelModel.forEach(item => { %>
                <li ID="Sheets_<%=j%>" draggable="true" ondragstart="dragstart_handler(event)"><%= item %></li>
                <%       j++; %>
                <%     }); %>
            </ul>
        </div>
    </div>
</div>


<div class="whiteArea" id="main" class="main" ondrop="drop_handler(event)" ondragover="dragover_handler(event)">
    <h1>The white space for dragging the sheet</h1>
    <div class="mainDrag">

    </div>
</div>

<div id="TableFields">

</div>

<% include ../partials/footer %>